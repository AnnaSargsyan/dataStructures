#include <iostream>
#include <string>
using namespace std;


int main(){
    int b[10]={1,6,8,56,32,75,2,1,15,67};
    int i,j;
    do{
        for(i=0;i<10;i++){
            j=b[i+1];
            if (b[i+1]<b[i]){
                b[i+1]=b[i];
                b[i]=j;
            }
        }
    }
    while (b[0]<b[1]);
    
    for (i=0;i<10;i++){
    cout <<  b[i] << endl;
    }
    return 0;
}
